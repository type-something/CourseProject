<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <WebRootPath>$(MSBuildProjectDirectory)/webroot</WebRootPath>
    </PropertyGroup>
    <ItemGroup>
        <RuntimeHostConfigurationOption Include="WebRootPath" Value="$(WebRootPath)" />
    </ItemGroup>
</Project>